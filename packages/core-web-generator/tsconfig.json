{
	"compileOnSave": true,
	"compilerOptions": {
		"allowUnusedLabels": false,
		"lib": [
			"ES2020",
		],
		"module": "commonjs",
		"moduleResolution": "node",
		"outDir": "./lib/",
		"rootDir": "./src/",
		"sourceMap": true,
		"strict": true,
		"target": "ES2020",
		"allowJs": true,
		"declaration": true
	},
	"exclude": [
		"node_modules"
	],
	"include": [
		"src/**/*.ts"
	],
	"files": [
		"src/generate.ts"
	]
}
