{
  "name": "pages",
  "version": "0.3.21",
  "description": "generator for GitHub pages.",
  "license": "MIT",
  "main": "lib/pages.js",
  "private": true,
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrhenry/core-web.git"
  },
  "scripts": {
    "build-html": "node lib/landing/landing.js && node lib/browser-compat/browser-compat.js",
    "build-css": "node lib/css/css.js",
    "build-js": "webpack",
    "build-fonts": "cp -r ./lib/fonts/ ./dist/fonts/",
    "build-images": "cp -r ./lib/images/ ./dist/images/",
    "build": "yarn build-html && yarn build-css && yarn build-js && yarn build-fonts && yarn build-images",
    "dev-server": "cd ./dist && npx http-server"
  },
  "bugs": {
    "url": "https://github.com/mrhenry/core-web/issues"
  },
  "dependencies": {
    "@mdn/browser-compat-data": "^3.0.1",
    "polyfill-library": "^3.100.0"
  }
}
