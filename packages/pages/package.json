{
  "name": "@mrhenry/pages",
  "version": "0.4.0",
  "description": "generator for GitHub pages.",
  "license": "MIT",
  "main": "lib/pages.js",
  "private": true,
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build-html": "node lib/pages.js",
    "build-css": "node ./build-css.js",
    "build-js": "webpack",
    "build-fonts": "cp -r ./lib/fonts/ ./dist/fonts/",
    "build-images": "cp -r ./lib/images/ ./dist/images/",
    "build": "npm run build-html && npm run build-css && npm run build-js && npm run build-fonts && npm run build-images",
    "dev-server": "cd ./dist && npx http-server"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@mdn/browser-compat-data": "^3.0.1",
    "@mrhenry/babel-plugin-core-web": "^0.4.0",
    "@mrhenry/core-web": "^0.4.0",
    "babel-loader": "^8.2.2",
    "browserstack-runner": "^0.9.4",
    "core-js": "^3.8.1",
    "cssnano": "^4.1.10",
    "polyfill-library": "^3.100.0",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "postcss-preset-env": "^6.7.0",
    "regenerator-runtime": "0.13.x",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.3.0"
  }
}
